[
    {
        "type": "add_custom_value_by_regex",
        "name": "funnyName",
        "from": "{team.fullName}",
        "rules": {
            "(.*)": "$1"
        }
    },
    {
        "type": "override_team_template",
        "medias": {
            "achievement": {
                "type": "Image",
                "url": "/media/{team.id}.svg"
            },
            "photo": {
                "type": "Image",
                "url": "/media/teams/{team.id}/photo.jpg"
            }
        }
    },
    {
        "type": "override_problems",
        "rules": {
            "A-sum-integers-SGQPRC": {
                "isHidden": true
            }
        }
    },
    {
        "type": "override_awards",
        "settings": {
            "medals": [
                {
                    "id": "gold-medal",
                    "citation": "Gold Medal",
                    "color": "GOLD",
                    "maxRank": 1
                },
                {
                    "id": "silver-medal",
                    "citation": "Solver Medal",
                    "color": "SILVER",
                    "maxRank": 3
                },
                {
                    "id": "bronze-medal",
                    "citation": "Bronze Medal",
                    "color": "BRONZE",
                    "maxRank": 6
                }
            ]
        }
    }
]