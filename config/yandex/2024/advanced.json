[
    {
        "type": "overrideTeamTemplate",
        "regexes": {
            "custom": {
                "from": "{team.fullName}",
                "rules": {
                    "(.*)": {
                        "funnyNameValue": "$1"
                    }
                }
            }
        },
        "customFields": {
            "funnyName": "{regexes.custom.funnyNameValue}"
        }
    },
    {
        "type": "overrideTeamTemplate",
        "regexes": {
            "group": {
                "from": "{team.id}",
                "rules": {
                    "^STAFF.*": {
                        "id": "staff"
                    },
                    "^JUNIOR.*": {
                        "id": "junior"
                    },
                    "^MAIN.*": {
                        "id": "main"
                    }
                }
            },
            "custom": {
                "from": "{team.id}",
                "rules": {
                    "^MAIN.*": {
                        "emojiValue": ""
                    },
                    "^STAFF.*": {
                        "emojiValue": "üë®‚Äçüíº"
                    },
                    "^JUNIOR.*": {
                        "emojiValue": "üë∂"
                    }
                }
            }
        },
        "extraGroups": [
            "{regexes.group.id}"
        ],
        "customFields": {
            "emoji": "{regexes.custom.emojiValue}"
        }
    },
    {
        "type": "overrideTeamTemplate",
        "displayName": "{emoji} {funnyName}",
        "medias": {
            "achievement": {
                "type": "Object",
                "url": "/media/achievements/{team.id}.svg"
            },
            "screen": {
                "type": "WebRTCGrabberConnection",
                "url": "https://yandex.live.icpc.global",
                "peerName": "{peer}",
                "streamType": "desktop",
                "credential": "pass"
            },
            "camera": {
                "type": "WebRTCGrabberConnection",
                "url": "https://yandex.live.icpc.global",
                "peerName": "{peer}",
                "streamType": "webcam",
                "credential": "pass"
            }
        }
    },
