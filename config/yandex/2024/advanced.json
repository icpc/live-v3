[
    {
        "type": "add_custom_value_by_regex",
        "name": "funnyName",
        "from": "{team.fullName}",
        "rules": {
            "(.*)": "$1"
        }
    },
    {
        "type": "add_group_if_matches",
        "from": "{team.id}",
        "rule": "^STAFF.*",
        "id": "staff"
    },
    {
        "type": "add_group_if_matches",
        "from": "{team.id}",
        "rule": "^JUNIOR.*",
        "id": "junior"
    },
    {
        "type": "add_group_if_matches",
        "from": "{team.id}",
        "rule": "^MAIN.*",
        "id": "main"
    },
    {
        "type": "add_custom_value_by_regex",
        "name": "emoji",
        "from": "{team.id}",
        "rules": {
            "^MAIN.*": "",
            "^STAFF.*": "üë®‚Äçüíº",
            "^JUNIOR.*": "üë∂"
        }
    },
    {
        "type": "override_team_template",
        "displayName": "{emoji} {funnyName}",
        "medias": {
            "achievement": {
                "type": "Object",
                "url": "/media/achievements/{team.id}.svg"
            },
            "screen": {
                "type": "WebRTCGrabberConnection",
                "url": "https://yandex.live.icpc.global",
                "peerName": "{peer}",
                "streamType": "desktop",
                "credential": "pass"
            },
            "camera": {
                "type": "WebRTCGrabberConnection",
                "url": "https://yandex.live.icpc.global",
                "peerName": "{peer}",
                "streamType": "webcam",
                "credential": "pass"
            }
        }
    },
