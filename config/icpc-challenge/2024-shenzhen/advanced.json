[
    {
        "type": "override_scoreboard_settings",
        "showTeamsWithoutSubmissions": false
    },
    {
        "type": "add_group_if_matches",
        "id": "team",
        "from": "{team.id}",
        "rule": "^.*team.*"
    },
    {
        "type": "add_group_if_matches",
        "id": "coach",
        "from": "{team.id}",
        "rule": "^.*coach.*"
    },
    {
        "type": "override_team_display_names",
        "rules": {
            "g30428=team46039": "46: IIT (ISM), Dhanbad"
        }
    },
    {
        "type": "override_problem_colors",
        "rules": {
            "0": "#cf0a2cff"
        }
    },
    {
        "type": "override_queue",
        "maxQueueSize": 20,
        "maxUntestedRun": 5
    }
]