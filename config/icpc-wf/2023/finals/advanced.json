[
    {
        "type": "override_team_template",
        "displayName": "{org.displayName}",
        "medias": {
            "achievement": {
                "type": "Image",
                "url": "https://arc.live.kbats.ru/contests/wf47_finals/teams/achievement/{team.id}.svg"
            },
            "photo": {
                "type": "Image",
                "url": "https://arc.live.kbats.ru/contests/wf47_finals/teams/team_photos/team-{team.id}.jpg"
            },
            "record": {
                "type": "Video",
                "url": "https://arc.live.kbats.ru/contests/wf47_finals/teams/team_videos/team-{team.id}.MP4"
            },
            "camera": {
                "type": "M2tsVideo",
                "url": "https://arc.live.kbats.ru/mock-stream/mpeg-ts?background=webcam.mp4&top_text={org.displayName}"
            },
            "screen": {
                "type": "M2tsVideo",
                "url": "https://arc.live.kbats.ru/mock-stream/mpeg-ts?background=desktop.mp4&top_text={org.displayName}"
            }
        }
    },
    {
        "type": "override_team_display_names",
        "rules": {}
    },
    {
        "type": "override_awards",
        "settings": {
            "medals": [
                {
                    "id": "gold-medal",
                    "citation": "Gold Medal",
