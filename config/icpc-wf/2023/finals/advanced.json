                {
                    "id": "silver-medal",
                    "citation": "Solver Medal",
                    "color": "SILVER",
                    "maxRank": 8
                },
                {
                    "id": "bronze-medal",
                    "citation": "Bronze Medal",
                    "color": "BRONZE",
                    "maxRank": 12
                }
            ]
        }
    }
