[
    {
        "type": "override_times",
        "startTime": "2023-12-12 08:10:00 +01:00",
        "freezeTimeSeconds": 14400
    },
    {
        "type": "override_scoreboard_settings",
        "showTeamsWithoutSubmissions": true
    },
    {
        "type": "set_organization_by_regex",
        "from": "{team.fullName}",
        "rules": {
            "^([^:]*).*": "$1"
        }
    },
    {
        "type": "add_custom_value_by_regex",
        "name": "funnyName",
        "from": "{team.fullName}",
        "rules": {
            "[^:]*: (.*) \\([^)]*\\)": "$1"
        }
    },
    {
        "type": "add_group_if_matches",
        "id": "local",
        "from": "{team.id}",
        "rule": "spb(\\d\\d\\d)"
    },
    {
        "type": "add_group_if_matches",
        "id": "remote",
        "from": "{team.id}",
        "rule": "[^s][^p][^b](\\d\\d\\d)"
    },
    {
        "type": "add_custom_value_by_regex",
        "name": "grabberPeerName",
        "from": "{team.id}",
        "rules": {
            "spb(\\d\\d\\d)": "$1",
            "geo(\\d\\d\\d)": "G$1",
            "nsk(\\d\\d\\d)": "N$1",
            "kaz(\\d\\d\\d)": "K$1"
        }
    },
    {
        "type": "add_custom_value_by_regex",
        "name": "grabberIp",
        "from": "{team.id}",
        "rules": {
            "spb(\\d\\d\\d)": "192.168.0.112",
            "geo(\\d\\d\\d)": "192.168.0.175",
            "nsk(\\d\\d\\d)": "192.168.0.175",
            "kaz(\\d\\d\\d)": "192.168.0.175"
        }
    },
    {
        "type": "override_team_template",
        "displayName": "{orgDisplayName} {funnyName}",
        "medias": {
            "achievement": {
                "type": "Image",
                "url": "/media/ach/{teamId}.svg"
            },
            "screen": {
                "type": "WebRTCGrabberConnection",
                "url": "http://{grabberIp}:8080",
                "peerName": "{grabberPeerName}",
                "streamType": "desktop",
                "credential": "live"
            },
            "camera": {
                "type": "WebRTCGrabberConnection",
                "url": "http://{grabberIp}:8080",
                "peerName": "{grabberPeerName}",
                "streamType": "webcam",
                "credential": "live"
            }
        }
    },
    {
        "type": "override_problem_colors",
        "rules": {
            "A": "#fe0000ff",
            "B": "#761946ff",
            "C": "#45bec6ff",
            "D": "#ffff00ff",
            "E": "#fe9900ff",
            "F": "#ff00feff",
            "G": "#e9d1deff",
            "H": "#38771eff",
            "I": "#ffffffff",
            "J": "#9b01ffff",
            "K": "#04ff00ff",
            "L": "#000000ff",
            "M": "#0000feff",
            "X": "#808080ff",
            "Y": "#808080ff",
            "Z": "#808080ff"
        }
    },
    {
        "type": "add_medals",
        "gold": 4,
        "silver": 5,
        "bronze": 6
    },
    {
        "type": "add_manual_award",
        "id": "qualified",
        "citation": "Qualified to world finals",
        "teamIds": []
    },
    {
        "type": "override_awards",
        "championTitle": "Northern Eurasia Finals Champions",
        "rankAwardsMaxRank": 13
    }
]