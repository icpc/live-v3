    {
        "type": "overrideTeamTemplate",
        "regexes": {
            "org": {
                "from": "{team.fullName}",
                "rules": {
                    "^([^:]*).*": {
                        "id": "$1"
                    }
                }
            },
            "custom": {
                "from": "{team.fullName}",
                "rules": {
                    "[^:]*: (.*) \\([^)]*\\)": {
                        "funnyNameValue": "$1"
                    }
                }
            }
        },
        "organizationId": "{regexes.org.id}",
        "customFields": {
            "funnyName": "{regexes.custom.funnyNameValue}"
        }
    },
