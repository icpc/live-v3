[
    {
        "type": "overrideContestSettings",
        "freezeTimeSeconds": 14400,
        "customFields": {
            "Test custom field": "test custom value"
        }
    },
    {
        "type": "overrideScoreboardSettings",
        "showTeamsWithoutSubmissions": true,
        "problemColorPolicy": {
            "type": "whenSolved",
            "colorBeforeSolved": "#808080ff"
        }
    },
    {
        "type": "overrideTeamTemplate",
        "regexes": {
            "org": {
                "from": "{team.fullName}",
                "rules": {
                    "^([^:]*).*": {
                        "id": "$1"
                    }
                }
            },
            "custom": {
                "from": "{team.fullName}",
                "rules": {
                    "[^:]*: (.*) \\([^)]*\\)": {
                        "funnyNameValue": "$1"
                    }
                }
            }
        },
        "organizationId": "{regexes.org.id}",
        "customFields": {
            "funnyName": "{regexes.custom.funnyNameValue}"
        }
    },
    {
        "type": "overrideTeamTemplate",
        "regexes": {
            "group": {
                "from": "{team.id}",
                "rules": {
                    "spb(\\d\\d\\d)": {
                        "id": "local"
                    },
                    "[^s][^p][^b](\\d\\d\\d)": {
                        "id": "remote"
                    }
                }
            },
            "custom": {
                "from": "{team.id}",
                "rules": {
                    "spb(\\d\\d\\d)": {
                        "grabberPeerNameValue": "$1",
                        "grabberIpValue": "192.168.0.112"
                    },
                    "geo(\\d\\d\\d)": {
                        "grabberPeerNameValue": "G$1",
                        "grabberIpValue": "192.168.0.175"
                    },
                    "nsk(\\d\\d\\d)": {
                        "grabberPeerNameValue": "N$1",
                        "grabberIpValue": "192.168.0.175"
                    },
                    "kaz(\\d\\d\\d)": {
                        "grabberPeerNameValue": "K$1",
                        "grabberIpValue": "192.168.0.175"
                    }
                }
            }
        },
        "extraGroups": [
            "{regexes.group.id}"
        ],
        "customFields": {
            "grabberPeerName": "{regexes.custom.grabberPeerNameValue}",
            "grabberIp": "{regexes.custom.grabberIpValue}"
        }
    },
    {
        "type": "overrideTeamTemplate",
        "displayName": "{org.displayName} {funnyName}",
        "hashTag": "{team.id}",
        "medias": {
            "achievement": {
                "type": "Image",
                "url": "/media/ach/{team.id}.svg"
            },
            "photo": {
                "type": "Image",
                "mime": "image/svg+xml;charset=utf-8",
                "url": "/media/ach/{team.id}.svg"
            },
            "screen": {
                "type": "WebRTCGrabberConnection",
                "url": "http://{grabberIp}:8080",
                "peerName": "{grabberPeerName}",
                "streamType": "desktop",
                "credential": "live"
            },
            "camera": {
                "type": "WebRTCGrabberConnection",
                "url": "http://{grabberIp}:8080",
                "peerName": "{grabberPeerName}",
                "streamType": "webcam",
                "credential": "live"
            }
        },
      "reactionVideoTemplate": [
        {
          "type": "Video",
          "url": "http://{grabberIp}:8080/reactions/{team.id}/camera{run.id}.mp4"
        },
        {
          "type": "Video",
          "url": "http://{grabberIp}:8080/reactions/{team.id}/desktop{run.id}.mp4"
        }
      ],
      "sourceTemplate": [
        {
          "type": "ZipArchive",
          "url": "http://{grabberIp}:8080/sources/{team.id}/source.{run.id}.zip"
        }
      ]
    },
    {
        "type": "overrideProblems",
        "rules": {
            "A": {"color": "#ffff00ff"},
            "B": {"color": "#ff00feff"},
            "C": {"color": "#761946ff"},
            "D": {"color": "#fe9900ff"},
            "E": {"color": "#0000feff"},
            "F": {"color": "#000000ff"},
            "G": {"color": "#04ff00ff"},
            "H": {"color": "#38771eff"},
            "I": {"color": "#9b01ffff"},
            "J": {"color": "#e9d1deff"},
            "K": {"color": "#ffffffff"},
            "L": {"color": "#fe0000ff"}
        }
    },
    {
        "type": "addMedals",
        "gold": 4,
        "silver": 4,
        "bronze": 4
    },
    {
        "type": "addKnownAwards",
        "championTitle": "Northern Eurasia Finals Champions",
        "rankAwardsMaxRank": 13
    },
    {
        "type": "addAwardChain",
        "awards": [
            {
                "id": "qualified",
                "citation": "Qualified to world finals",
                "manualTeamIds": []
            }
        ]
    },
    {
        "type": "overrideQueue",
        "waitTimeSeconds": 60,
        "firstToSolveWaitTimeSeconds": 120,
        "featuredRunWaitTimeSeconds": 60,
        "inProgressRunWaitTimeSeconds": 300,
        "maxQueueSize": 15,
        "maxUntestedRun": 5
    }
]