[
    {
        "type": "override_times",
        "freezeTimeSeconds": 14400
    },
    {
        "type": "override_scoreboard_settings",
        "showTeamsWithoutSubmissions": true
    },
    {
        "type": "set_organization_by_regex",
        "from": "{team.fullName}",
        "rules": {
            "^([^:]*).*": "$1"
        }
    },
    {
        "type": "add_custom_value_by_regex",
        "name": "funnyName",
        "from": "{team.fullName}",
        "rules": {
            "[^:]*: (.*) \\([^)]*\\)": "$1"
        }
    },
    {
        "type": "add_group_if_matches",
        "from": "{team.id}",
        "rule": "spb(\\d\\d\\d)",
        "id": "local"
    },
    {
        "type": "add_group_if_matches",
        "from": "{team.id}",
        "rule": "[^s][^p][^b](\\d\\d\\d)",
        "id": "remote"
    },
    {
        "type": "add_custom_value_by_regex",
        "name": "grabberPeerName",
        "from": "{team.id}",
        "rules": {
            "spb(\\d\\d\\d)": "$1",
            "geo(\\d\\d\\d)": "G$1",
            "nsk(\\d\\d\\d)": "N$1",
            "kaz(\\d\\d\\d)": "K$1"
        }
    },
    {
        "type": "add_custom_value_by_regex",
        "name": "grabberIp",
        "from": "{team.id}",
        "rules": {
            "spb(\\d\\d\\d)": "192.168.0.112",
            "geo(\\d\\d\\d)": "192.168.0.175",
            "nsk(\\d\\d\\d)": "192.168.0.175",
            "kaz(\\d\\d\\d)": "192.168.0.175"
        }
    },
    {
        "type": "override_team_template",
        "displayName": "{org.displayName} {funnyName}",
        "hashTag": "{team.id}",
        "medias": {
            "achievement": {
                "type": "Image",
                "url": "/media/ach/{team.id}.svg"
            },
            "photo": {
                "type": "Image",
                "url": "/media/ach/{team.id}.svg"
            },
            "screen": {
                "type": "WebRTCGrabberConnection",
                "url": "http://{grabberIp}:8080",
                "peerName": "{grabberPeerName}",
                "streamType": "desktop",
                "credential": "live"
            },
            "camera": {
                "type": "WebRTCGrabberConnection",
                "url": "{grabberUrl}",
                "peerName": "http://{grabberIp}:8080",
                "streamType": "webcam",
                "credential": "live"
            }
        }
    },
    {
        "type": "override_problems",
        "rules": {
            "A": {
                "color": "#ffff00ff",
                "unsolvedColor": "#808080ff"
            },
            "B": {
                "color": "#ff00feff",
                "unsolvedColor": "#808080ff"
            },
            "C": {
                "color": "#761946ff",
                "unsolvedColor": "#808080ff"
            },
            "D": {
                "color": "#fe9900ff",
                "unsolvedColor": "#808080ff"
            },
            "E": {
                "color": "#0000feff",
                "unsolvedColor": "#808080ff"
            },
            "F": {
                "color": "#000000ff",
                "unsolvedColor": "#808080ff"
            },
            "G": {
                "color": "#04ff00ff",
                "unsolvedColor": "#808080ff"
            },
            "H": {
                "color": "#38771eff",
                "unsolvedColor": "#808080ff"
            },
            "I": {
                "color": "#9b01ffff",
                "unsolvedColor": "#808080ff"
            },
            "J": {
                "color": "#e9d1deff",
                "unsolvedColor": "#808080ff"
            },
            "K": {
                "color": "#ffffffff",
                "unsolvedColor": "#808080ff"
            },
            "L": {
                "color": "#fe0000ff",
                "unsolvedColor": "#808080ff"
            }
        }
    },
    {
        "type": "override_awards",
        "settings": {
            "championTitle": "Northern Eurasia Finals Champions",
            "rankAwardsMaxRank": 13,
            "medals": [
                {
                    "id": "gold-medal",
                    "citation": "Gold Medal",
                    "color": "GOLD",
                    "maxRank": 4
                },
                {
                    "id": "silver-medal",
                    "citation": "Silver Medal",
                    "color": "SILVER",
                    "maxRank": 8
                },
                {
                    "id": "bronze-medal",
                    "citation": "Bronze Medal",
                    "color": "BRONZE",
                    "maxRank": 12
                }
            ],
            "manual": [
                {
                    "id": "qualified",
                    "citation": "Qualified to world finals",
                    "teamCdsIds": []
                }
            ]
        }
    },
    {
        "type": "override_queue",
        "waitTimeSeconds": 60,
        "firstToSolveWaitTimeSeconds": 120,
        "featuredRunWaitTimeSeconds": 60,
        "inProgressRunWaitTimeSeconds": 300,
        "maxQueueSize": 15,
        "maxUntestedRun": 5
    }
]