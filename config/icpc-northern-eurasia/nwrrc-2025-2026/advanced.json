[
  {
    "type": "overrideOrganizationTemplate",
    "logo": {
      "type": "Image",
      "url": "/media/organizations/{org.id}/logo.svg"
    }
  },
  {
    "type": "overrideTeamTemplate",
    "regexes": {
      "fromName": {
        "from": "{team.fullName}",
        "rules": {
          "([^:]*): (.*) (\\([^)]*\\))": {
            "funnyNameValue": "$2",
            "contestants": "$3"
          }
        }
      },
      "fromId": {
        "from": "{team.id}",
        "rules": {
          "spb(.*)": {"compValue": "$1"}
        }
      }
    },
    "customFields": {
      "funnyName": "{regexes.fromName.funnyNameValue}",
      "comp": "{regexes.fromId.compValue}"
    }
  },
  {
    "type": "overrideTeamTemplate",
    "displayName": "{org.displayName}: {funnyName}",
    "fullName": "{org.fullName}: {funnyName}",
    "medias": {
      "screen": {
        "type": "WebRTCGrabberConnection",
        "url": "https://grabber.com",
        "peerName": "{comp}",
        "streamType": "desktop",
        "credential": "live"
      },
      "camera": {
        "type": "WebRTCGrabberConnection",
        "url": "https://grabber.com",
        "peerName": "{comp}",
        "streamType": "webcam",
        "credential": "live"
      },
      "achievement": {
        "type": "Object",
        "url": "/api/overlay/svgAchievement/ach-smart-with-logo.svg?teamId={team.id}"
      }
    }
  },
  {
    "type": "overrideOrganizations",
    "rules": {
      "3295": {"customFields": {"qualifiedLimit": "6"}},
      "1802": {"customFields": {"qualifiedLimit": "5"}},
      "1456": {"customFields": {"qualifiedLimit": "3"}},
      "1584": {"customFields": {"qualifiedLimit": "3"}}
    }
  },
  {
    "type": "addAwardChain",
    "awards": [
      {
        "id": "qualified",
        "citation": "Qualified to Northern Eurasia Finals",
        "limit": 23,
        "minScore": 0,
        "organizationLimit": 2,
        "organizationLimitCustomField": "qualifiedLimit"
      }
    ]
  },
  {
    "type": "overrideScoreboardSettings",
    "problemColorPolicy": {
      "type": "whenSolved",
      "colorBeforeSolved": "#808080"
    }
  },
  {
    "type": "overrideProblems",
    "rules": {
      "A": { "color": "#C28039" },
      "B": { "color": "#3E8DC2" },
      "C": { "color": "#b94743" },
      "D": { "color": "#7FB26A" },
      "E": { "color": "#53724B" },
      "F": { "color": "#D1C962" },
      "G": { "color": "#393839" },
      "H": { "color": "#D3557E" },
      "I": { "color": "#714A4C" },
      "J": { "color": "#3EABB4" },
      "K": { "color": "#000000" },
      "L": { "color": "#8E74B7" },

      "W": { "color": "#a6406e" },
      "X": { "color": "#bb7190" },
      "Y": { "color": "#3daa91" },
      "Z": { "color": "#3f4a58" }
    }
  }
]
