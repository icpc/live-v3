[
    {
        "type": "add_group_if_matches",
        "from": "{team.fullName}",
        "rule": "^\\(вк\\).*",
        "id": "outOfContest"
    },
    {
        "type": "add_group_if_matches",
        "from": "{team.fullName}",
        "rule": "^\\(1к\\).*",
        "id": "firstGrade"
    },
    {
        "type": "add_group_if_matches",
        "from": "{team.fullName}",
        "rule": "^\\(шк\\).*",
        "id": "school"
    },
    {
        "type": "add_custom_value_by_regex",
        "name": "funnyName",
        "from": "{team.fullName}",
        "rules": {
            "^(?:\\(..\\) )?(.*) \\([^)]*\\)": "$1"
        }
    },
    {
        "type": "override_groups",
        "rules": {
            "outOfContest": {
                "isOutOfContest": true
            }
        }
    },
    {
        "type": "override_team_template",
        "displayName": "{funnyName}"
    },
    {
        "type": "override_problems",
        "rules": {
            "A": {
                "ordinal": 100
            }
        }
    }
]