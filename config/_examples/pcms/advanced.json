[
    {
        "type": "add_group_if_matches",
        "id": "itmo",
        "from": "{team.fullName}",
        "rule": "^SPb ITMO:.*"
    },
    {
        "type": "add_group_if_matches",
        "id": "spbsu",
        "from": "{team.fullName}",
        "rule": "^SPb SU:.*"
    },
    {
        "type": "add_group_if_matches",
        "id": "leti",
        "from": "{team.fullName}",
        "rule": "^SPb ETU:.*"
    },
    {
        "type": "add_custom_value_by_regex",
        "name": "shortName",
        "from": "{team.fullName}",
        "rules": {
            "^(?:\\(..\\) )?(.*) \\([^)]*\\)": "$1"
        }
    },
    {
        "type": "add_custom_value_by_regex",
        "name": "icpc_id",
        "from": "{team.id}",
        "rules": {
            "^([0-9]*)$": "$1"
        }
    },
    {
        "type": "override_team_template",
        "displayName": "{shortName}"
    },
    {
        "type": "override_team_display_names",
        "rules": {}
    },
    {
        "type": "override_problem_colors",
        "rules": {
            "A": "#e6194bff",
            "B": "#3cb44bff",
            "C": "#ffe119ff",
            "D": "#4363d8ff",
            "E": "#f58231ff",
            "F": "#42d4f4ff",
            "G": "#f032e6ff",
            "H": "#fabed4ff",
            "I": "#469990ff",
            "J": "#dcbeffff",
            "K": "#9a6324ff",
            "L": "#fffac8ff"
        }
    },
    {
        "type": "add_manual_award",
        "id": "qualified",
        "citation": "Qualified to Northwestern Russia Regional Contest",
        "teamIds": [
            "922155"
        ]
    }
]